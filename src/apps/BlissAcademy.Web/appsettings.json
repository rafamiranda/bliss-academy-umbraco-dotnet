{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "MacroErrors": "Inline"
      },
      "Global": {
        "Id": "2071381e-d0c1-4b88-8059-82136237d17b"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "BlissAcademy.Core.PublishedContent",
        "ModelsDirectory": "~/../../../src/libs/BlissAcademy.Core/PublishedContent",
        "AcceptUnsafeModelsDirectory": true
      },
      "WebRouting": {
        "TrySkipIisCustomErrors": true
      }
    }
  },
  "DatabaseInitialization": {
    "BlissAcademy": {
      "MigrationsEnabled": true,
      "SeedType": "None"
    }
  }
}